{
    "name": "swancreative/laravel-wp-api",
    "description": "Laravel package for the Wordpress JSON REST API forked from threesquared",
    "keywords": ["laravel", "package", "wordpress", "API", "REST"],
    "license": "MIT",
    "authors": [
        {
            "name": "Ben Speakman/Matt Wilding",
            "email": "support@swancreative.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/support": "^5.0",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*"
    },
    "autoload": {
        "psr-0": {
            "Swancreative\\LaravelWpApi\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Swancreative\\LaravelWpApi\\LaravelWpApiServiceProvider"
            ]
        }
    }
}
